Automation Test
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;

public class Join {
	 WebDriver driver;
		@BeforeTest
		public void lauchingBrowser()
		{
			driver=new ChromeDriver();
			driver.get("http://www.goinglobal.com/demo-registration-form-november-16-2018-0");
		}
		@Test(priority=1)
		public void Register()
		{
			driver.findElement(By.xpath("//input[@name='submitted[name]']")).sendKeys("ameen");
			driver.findElement(By.xpath("//input[@name='submitted[phone]']")).sendKeys("0835927451");
			driver.findElement(By.xpath("//input[@id='edit-submitted-organization-university']")).sendKeys("Madurai kamaraj university");
		}
		@Test(priority=2)
		public void details()
		{
			driver.findElement(By.xpath("//input[@name='submitted[department]']")).sendKeys("IT");
			driver.findElement(By.xpath("//input[@name='submitted[title]']")).sendKeys("Mr");
			driver.findElement(By.xpath("//input[@id='edit-submitted-e-mail-address']")).sendKeys("ameenmohamed747@gmail.com");
		}
		@Test(priority=3)
		public void Title()
		{
			driver.findElement(By.xpath("//textarea[@name='submitted[comments]']")).sendKeys("hello,");
			driver.findElement(By.xpath("//input[@class='webform-submit button-primary form-submit']")).click();
     }
			@AfterTest
		public void close()
		{
			driver.quit();
			
			

}}
